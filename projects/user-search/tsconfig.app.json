{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@user-search-app/*": ["./src/app/app/*"],
      "@user-search-core/*": ["./src/app/_core/*"],
      "@user-search-environments/*": ["./src/environments/*"],
      "@user-search-shared/*": ["./src/app/_shared/*"],
      "@user-search-search/*": ["./src/app/search/*"],
      "@github-search/ng-kit": ["../../dist/@github-search/ng-kit"],
      "@github-search/test-kit": ["../../dist/@github-search/test-kit"]
    }
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": "off"
  },
  "exclude": ["**/*.spec.ts"]
}
